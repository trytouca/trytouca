{
  "artifactory": {
    "artifact_url": "https://touca.jfrog.io/artifactory/{repo}/{group}/{name}/{name}-{version}.tar.gz",
    "repo": "example-repo-local",
    "group": "io.touca",
    "name": "sample_app"
  },
  "install": {
    "installer": "sample_app/install.sh",
    "destination": "./deployed"
  },
  "execution": {
    "executable": "sample_app",
    "output-directory": "./output",
    "suite": "sample_app"
  },
  "archive": {
    "dir": "./archive"
  }
}
