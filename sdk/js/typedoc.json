{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Touca SDK for JavaScript",
  "entryPoints": ["./packages/node/src"],
  "entryPointStrategy": "expand",
  "out": "./local/docs",
  "readme": "README.md",
  "includes": "./",
  "exclude": [
    "**/*.js",
    "**/coverage/**/*",
    "**/dist/**/*",
    "**/node_modules/**/*",
    "**/test/**/*"
  ],
  "excludeExternals": true,
  "disableSources": true,
  "excludePrivate": true,
  "hideGenerator": true,
  "includeVersion": true,
  "sort": ["visibility", "static-first", "source-order"],
  "theme": "default"
}
