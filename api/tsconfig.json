{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/controllers/*": ["controllers/*.js"],
      "@/middlewares": ["middlewares/index.js"],
      "@/models/*": ["models/*.js"],
      "@/queues": ["queues/index.js"],
      "@/routes/*": ["routes/*.js"],
      "@/schemas/*": ["schemas/*.js"],
      "@/types/*": ["types/*.js"],
      "@/utils/*": ["utils/*.js"]
    },
    "alwaysStrict": true,
    "outDir": "dist",
    "sourceMap": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "esModuleInterop": true,
    "noImplicitAny": false
  },
  "include": ["src/**/*"]
}
