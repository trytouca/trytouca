{
  "name": "@touca/api",
  "version": "1.4.0",
  "description": "Continuous regression testing for critical software workflows.",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/trytouca/touca.git",
  "bugs": "https://github.com/trytouca/touca/issues",
  "homepage": "https://touca.io",
  "keywords": [
    "snapshot-testing",
    "test-automation",
    "test-framework",
    "regression-testing"
  ],
  "author": {
    "name": "Pejman Ghorbanzade",
    "email": "pejman@touca.io"
  },
  "main": "dist/server.js",
  "scripts": {
    "build": "yarn run build:mails && yarn run build:ts",
    "build:mails": "mkdir -p dist/templates; mjml templates/*.mjml --config.beautify false --config.minify true --output ./dist/templates/; cp -r templates/reports dist/templates",
    "build:ts": "tsc",
    "clean": "rimraf dist",
    "docs:external": "mkdir -p local/docs/external; swagger-inline src/routes/*.ts src/types/commontypes.ts --base docs/swagger.base.yaml --format yaml > local/docs/external/swagger.yaml; redoc-cli build local/docs/external/swagger.yaml --options.hideDownloadButton=true --output=local/docs/external/index.html --title=\"Touca Server API Reference\"",
    "docs:internal": "typedoc",
    "docs": "yarn run docs:external && yarn run docs:internal",
    "start": "node .",
    "test": "cross-env ENV_FILE=test jest",
    "lint": "eslint src test --ext .ts",
    "dev": "nodemon ./src/server.ts",
    "dev:debug": "nodemon --inspect ./src/server.ts"
  },
  "dependencies": {
    "@hubspot/api-client": "^7.0.1",
    "@sindresorhus/df": "^3.1.1",
    "archiver": "^5.3.1",
    "aws-sdk": "^2.1167.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "flatbuffers": "^2.0.6",
    "google-auth-library": "^8.1.0",
    "hide-powered-by": "^1.0.0",
    "html-to-text": "^8.2.0",
    "ioredis": "^5.1.0",
    "jws": "^4.0.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.17",
    "minio": "^7.0.28",
    "mixpanel": "^0.16.0",
    "module-alias": "^2.2.2",
    "mongoose": "^6.4.2",
    "mustache": "^4.2.0",
    "nocache": "^3.0.4",
    "nodemailer": "^6.7.6",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/flatbuffers": "^1.9.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^28.1.4",
    "@types/jws": "^3.2.4",
    "@types/lodash": "^4.14.182",
    "@types/marked": "^4.0.3",
    "@types/minio": "^7.0.13",
    "@types/mustache": "^4.1.3",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/sinon": "^10.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^28.1.2",
    "mjml": "^4.12.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "redoc-cli": "^0.13.16",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "swagger-inline": "^6.0.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typedoc": "^0.23.4",
    "typescript": "^4.6.4"
  }
}
